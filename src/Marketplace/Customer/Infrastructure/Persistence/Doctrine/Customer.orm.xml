<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="ShopSaas\Marketplace\Customer\Domain\Customer" table="customers">
        <id name="id" type="customer_id" column="id" length="36" />

        <embedded name="associated_id" class="ShopSaas\Marketplace\Customer\Domain\CustomerAssociatedId" use-column-prefix="false" />
        <embedded name="username" class="ShopSaas\Marketplace\Customer\Domain\CustomerUsername" use-column-prefix="false" />
    </entity>

</doctrine-mapping>
