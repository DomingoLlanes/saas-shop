<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="ShopSaas\Backoffice\Auth\Domain\AuthUser" table="auth_users">
        <id name="id" type="auth_id" column="id" length="36" />

        <embedded name="username" class="ShopSaas\Backoffice\Auth\Domain\AuthUsername" use-column-prefix="false" />
        <embedded name="password" class="ShopSaas\Backoffice\Auth\Domain\AuthPassword" use-column-prefix="false" />
    </entity>

</doctrine-mapping>
